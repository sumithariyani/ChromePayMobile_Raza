<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fuse_l"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@+id/back_img_linkServices"
    android:layout_marginTop="20dp"
    android:layout_marginBottom="10dp"
    android:background="@drawable/service_card_background"
    android:layout_marginHorizontal="10dp"
    android:elevation="10dp">

    <TextView
        android:id="@+id/fuse_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:text="Org Name"
        android:textSize="16dp"
        android:textStyle="bold"/>

    <LinearLayout
        android:id="@+id/bank_img_ls"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:elevation="5dp"
        android:layout_marginHorizontal="10dp"
        android:layout_marginVertical="10dp"
        android:background="@drawable/image_card_background">

        <ImageView
            android:id="@+id/dash_ic"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="5dp"
            android:src="@drawable/icon_12_1"/>
    </LinearLayout>


    <ImageView
        android:id="@+id/fuse_img"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bank_img_ls"
        android:layout_centerInParent="true"
        android:src="@drawable/image_2" />

    <TextView
        android:id="@+id/fuse_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fuse_img"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:text="@string/text_11" />

</RelativeLayout>